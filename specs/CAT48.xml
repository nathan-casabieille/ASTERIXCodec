          <Entry value="1" meaning="Garbled code"/>
        </Element>
        <Element name="L" bits="1" encoding="table">
          <Entry value="0" meaning="Mode-2 code as derived from the reply of the transponder"/>
          <Entry value="1" meaning="Smoothed Mode-2 code as provided by a local tracker"/>
        </Element>
        <Spare bits="1"/>
        <Element name="MODE2" bits="12" encoding="string_octal"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
