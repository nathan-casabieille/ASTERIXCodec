<?xml version="1.0" encoding="UTF-8"?>
<!--
  CAT48.xml – ASTERIX Category 048 "Monoradar Target Reports"
  Edition 1.32, 2024-07-01.

  Schema conventions (same as other categories):
    <Fixed>              – byte-aligned, fixed-length group.
    <Extended>           – variable octets, each <Octet> = 7 data bits + implicit FX LSB.
    <Repetitive type="fx">    – 7-bit values, FX-terminated.
    <Repetitive type="count"> – 1-byte REP count, then REP × <Group>.
    <Compound>           – PSF-driven sub-items, each sub-item a <Fixed> group.
                           PSF bit7=sub-item 0, bit6=sub-item 1, …, bit1=sub-item 6, bit0=FX.
    <Explicit type="re/sp">   – first byte is total length (incl. itself).

  Note on I048/020 octets 3–6: the spec groups sub-fields (EP+VAL pairs)
  into nested groups; they are flattened here to individual <Element>s because
  the codec's <Octet> only supports flat Element/Spare children.

  Note on I048/120 (Radial Doppler Speed): the RDS sub-item is a count-prefixed
  RepetitiveGroup inside a Compound, which the current codec does not support.
  The RDS PSF slot is therefore marked "-" (unused). CAL-only messages decode
  correctly; messages with RDS present are not supported.
-->
<Category cat="48"
          name="Monoradar Target Reports"
          edition="1.32"
          date="2024-07-01">

  <DataItems>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/010 – Data Source Identifier  (Fixed, 2 bytes)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="010" name="Data Source Identifier" presence="mandatory">
      <Fixed>
        <Element name="SAC" bits="8" encoding="raw"/>
        <Element name="SIC" bits="8" encoding="raw"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/020 – Target Report Descriptor  (Extended, up to 6 octets)
         Each <Octet> must contain exactly 7 bits.
         Octet 3–6 group sub-fields (EP+VAL) are flattened into individual
         elements; their original group names are encoded in the element names.
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="020" name="Target Report Descriptor">
      <Extended>
        <!-- Octet 1 (7 bits): TYP SIM RDP SPI RAB -->
        <Octet>
          <Element name="TYP" bits="3" encoding="table">
            <Entry value="0" meaning="No detection"/>
            <Entry value="1" meaning="Single PSR detection"/>
            <Entry value="2" meaning="Single SSR detection"/>
            <Entry value="3" meaning="SSR + PSR detection"/>
            <Entry value="4" meaning="Single ModeS All-Call"/>
            <Entry value="5" meaning="Single ModeS Roll-Call"/>
            <Entry value="6" meaning="ModeS All-Call + PSR"/>
            <Entry value="7" meaning="ModeS Roll-Call + PSR"/>
          </Element>
          <Element name="SIM" bits="1" encoding="table">
            <Entry value="0" meaning="Actual target report"/>
            <Entry value="1" meaning="Simulated target report"/>
          </Element>
          <Element name="RDP" bits="1" encoding="table">
            <Entry value="0" meaning="Report from RDP Chain 1"/>
            <Entry value="1" meaning="Report from RDP Chain 2"/>
          </Element>
          <Element name="SPI" bits="1" encoding="table">
            <Entry value="0" meaning="Absence of SPI"/>
            <Entry value="1" meaning="Special Position Identification"/>
          </Element>
          <Element name="RAB" bits="1" encoding="table">
            <Entry value="0" meaning="Report from aircraft transponder"/>
            <Entry value="1" meaning="Report from field monitor (fixed transponder)"/>
          </Element>
        </Octet>
        <!-- Octet 2 (7 bits): TST ERR XPP ME MI FOEFRI -->
        <Octet>
          <Element name="TST" bits="1" encoding="table">
            <Entry value="0" meaning="Real target report"/>
            <Entry value="1" meaning="Test target report"/>
          </Element>
          <Element name="ERR" bits="1" encoding="table">
            <Entry value="0" meaning="No Extended Range"/>
            <Entry value="1" meaning="Extended Range present"/>
          </Element>
          <Element name="XPP" bits="1" encoding="table">
            <Entry value="0" meaning="No X-Pulse present"/>
            <Entry value="1" meaning="X-Pulse present"/>
          </Element>
          <Element name="ME" bits="1" encoding="table">
            <Entry value="0" meaning="No military emergency"/>
            <Entry value="1" meaning="Military emergency"/>
          </Element>
          <Element name="MI" bits="1" encoding="table">
            <Entry value="0" meaning="No military identification"/>
            <Entry value="1" meaning="Military identification"/>
          </Element>
          <Element name="FOEFRI" bits="2" encoding="table">
            <Entry value="0" meaning="No Mode 4 interrogation"/>
            <Entry value="1" meaning="Friendly target"/>
            <Entry value="2" meaning="Unknown target"/>
            <Entry value="3" meaning="No reply"/>
          </Element>
        </Octet>
        <!-- Octet 3 (7 bits): ADSB (EP+VAL), SCN (EP+VAL), PAI (EP+VAL), spare -->
        <Octet>
          <Element name="ADSB_EP" bits="1" encoding="table">
            <Entry value="0" meaning="ADSB not populated"/>
            <Entry value="1" meaning="ADSB populated"/>
          </Element>
          <Element name="ADSB_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="Not available"/>
            <Entry value="1" meaning="Available"/>
          </Element>
          <Element name="SCN_EP" bits="1" encoding="table">
            <Entry value="0" meaning="SCN not populated"/>
            <Entry value="1" meaning="SCN populated"/>
          </Element>
          <Element name="SCN_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="Not available"/>
            <Entry value="1" meaning="Available"/>
          </Element>
          <Element name="PAI_EP" bits="1" encoding="table">
            <Entry value="0" meaning="PAI not populated"/>
            <Entry value="1" meaning="PAI populated"/>
          </Element>
          <Element name="PAI_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="Not available"/>
            <Entry value="1" meaning="Available"/>
          </Element>
          <Spare bits="1"/>
        </Octet>
        <!-- Octet 4 (7 bits): ACASXV (EP+VAL 4b), POXPR (EP+VAL) -->
        <Octet>
          <Element name="ACASXV_EP" bits="1" encoding="table">
            <Entry value="0" meaning="ACASXV not populated"/>
            <Entry value="1" meaning="ACASXV populated"/>
          </Element>
          <Element name="ACASXV_VAL" bits="4" encoding="table">
            <Entry value="0" meaning="Non-Extended Version"/>
            <Entry value="1" meaning="ACAS Xa Version 1"/>
            <Entry value="2" meaning="ACAS Xu Version 1"/>
          </Element>
          <Element name="POXPR_EP" bits="1" encoding="table">
            <Entry value="0" meaning="POXPR not populated"/>
            <Entry value="1" meaning="POXPR populated"/>
          </Element>
          <Element name="POXPR_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="PO not supported (PPM only)"/>
            <Entry value="1" meaning="PO supported"/>
          </Element>
        </Octet>
        <!-- Octet 5 (7 bits): POACT (EP+VAL), DTFXPR (EP+VAL), DTFACT (EP+VAL), spare -->
        <Octet>
          <Element name="POACT_EP" bits="1" encoding="table">
            <Entry value="0" meaning="POACT not populated"/>
            <Entry value="1" meaning="POACT populated"/>
          </Element>
          <Element name="POACT_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="PO not active"/>
            <Entry value="1" meaning="PO active"/>
          </Element>
          <Element name="DTFXPR_EP" bits="1" encoding="table">
            <Entry value="0" meaning="DTFXPR not populated"/>
            <Entry value="1" meaning="DTFXPR populated"/>
          </Element>
          <Element name="DTFXPR_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="Basic Dataflash not supported"/>
            <Entry value="1" meaning="Basic Dataflash supported"/>
          </Element>
          <Element name="DTFACT_EP" bits="1" encoding="table">
            <Entry value="0" meaning="DTFACT not populated"/>
            <Entry value="1" meaning="DTFACT populated"/>
          </Element>
          <Element name="DTFACT_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="Basic Dataflash not active"/>
            <Entry value="1" meaning="Basic Dataflash active"/>
          </Element>
          <Spare bits="1"/>
        </Octet>
        <!-- Octet 6 (7 bits): IRMXPR (EP+VAL), IRMACT (EP+VAL), spare 3 -->
        <Octet>
          <Element name="IRMXPR_EP" bits="1" encoding="table">
            <Entry value="0" meaning="IRMXPR not populated"/>
            <Entry value="1" meaning="IRMXPR populated"/>
          </Element>
          <Element name="IRMXPR_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="Transponder not IRM capable"/>
            <Entry value="1" meaning="Transponder IRM capable"/>
          </Element>
          <Element name="IRMACT_EP" bits="1" encoding="table">
            <Entry value="0" meaning="IRMACT not populated"/>
            <Entry value="1" meaning="IRMACT populated"/>
          </Element>
          <Element name="IRMACT_VAL" bits="1" encoding="table">
            <Entry value="0" meaning="IRM not active"/>
            <Entry value="1" meaning="IRM active"/>
          </Element>
          <Spare bits="3"/>
        </Octet>
      </Extended>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/030 – Warning/Error Conditions  (Repetitive FX, 7-bit table)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="030" name="Warning/Error Conditions and Target Classification">
      <Repetitive type="fx">
        <Element bits="7" encoding="table">
