          <Entry value="1"  meaning="Multipath Reply (Reflection)"/>
          <Entry value="2"  meaning="Reply due to sidelobe interrogation/reception"/>
          <Entry value="3"  meaning="Split plot"/>
          <Entry value="4"  meaning="Second time around reply"/>
          <Entry value="5"  meaning="Angel"/>
          <Entry value="6"  meaning="Slow moving target correlated with road infrastructure"/>
          <Entry value="7"  meaning="Fixed PSR plot"/>
          <Entry value="8"  meaning="Slow PSR target"/>
          <Entry value="9"  meaning="Low quality PSR plot"/>
          <Entry value="10" meaning="Phantom SSR plot"/>
          <Entry value="11" meaning="Non-Matching Mode-3/A Code"/>
          <Entry value="12" meaning="Mode C code / Mode S altitude code abnormal value"/>
          <Entry value="13" meaning="Target in Clutter Area"/>
          <Entry value="14" meaning="Maximum Doppler Response in Zero Filter"/>
          <Entry value="15" meaning="Transponder anomaly detected"/>
          <Entry value="16" meaning="Duplicated or Illegal Mode S Aircraft Address"/>
          <Entry value="17" meaning="Mode S error correction applied"/>
          <Entry value="18" meaning="Undecodable Mode C code / Mode S altitude code"/>
          <Entry value="19" meaning="Birds"/>
          <Entry value="20" meaning="Flock of Birds"/>
          <Entry value="21" meaning="Mode-1 was present in original reply"/>
          <Entry value="22" meaning="Mode-2 was present in original reply"/>
          <Entry value="23" meaning="Plot potentially caused by Wind Turbine"/>
          <Entry value="24" meaning="Helicopter"/>
          <Entry value="25" meaning="Maximum number of re-interrogations reached (surveillance)"/>
          <Entry value="26" meaning="Maximum number of re-interrogations reached (BDS)"/>
          <Entry value="27" meaning="BDS Overlay Incoherence"/>
          <Entry value="28" meaning="Potential BDS Swap Detected"/>
          <Entry value="29" meaning="Track Update in the Zenithal Gap"/>
          <Entry value="30" meaning="Mode S Track re-acquired"/>
          <Entry value="31" meaning="Duplicated Mode 5 Pair NO/PIN detected"/>
          <Entry value="32" meaning="Wrong DF reply format detected"/>
          <Entry value="33" meaning="Transponder anomaly (MS XPD replies with Mode A/C to Mode A/C-only all-call)"/>
          <Entry value="34" meaning="Transponder anomaly (SI capability report wrong)"/>
          <Entry value="35" meaning="Potential IC Conflict"/>
          <Entry value="36" meaning="IC Conflict detection possible - no conflict currently detected"/>
          <Entry value="37" meaning="Duplicate Mode 5 PIN"/>
        </Element>
      </Repetitive>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/040 – Measured Position in Polar Co-ordinates  (Fixed, 4 bytes)
         RHO:   LSB = 1/2^8 NM   (unsigned)
         THETA: LSB = 360/2^16 ° (unsigned)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="040" name="Measured Position in Polar Co-ordinates">
      <Fixed>
        <Element name="RHO"   bits="16" encoding="unsigned_quantity"
                 scale="0.00390625" unit="NM"/>
        <Element name="THETA" bits="16" encoding="unsigned_quantity"
                 scale="0.0054931640625" unit="deg"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/042 – Calculated Position in Cartesian Co-ordinates  (Fixed, 4 bytes)
         X, Y: signed, LSB = 1/2^7 NM
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="042" name="Calculated Position in Cartesian Co-ordinates">
      <Fixed>
        <Element name="X" bits="16" encoding="signed_quantity"
                 scale="0.0078125" unit="NM" min="-256" max="256"/>
        <Element name="Y" bits="16" encoding="signed_quantity"
                 scale="0.0078125" unit="NM" min="-256" max="256"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
