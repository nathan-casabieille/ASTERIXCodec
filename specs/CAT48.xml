        <Element name="QB1" bits="1" encoding="table">
          <Entry value="0" meaning="High quality pulse B1"/>
          <Entry value="1" meaning="Low quality pulse B1"/>
        </Element>
        <Element name="QD1" bits="1" encoding="table">
          <Entry value="0" meaning="High quality pulse D1"/>
          <Entry value="1" meaning="Low quality pulse D1"/>
        </Element>
        <Element name="QB2" bits="1" encoding="table">
          <Entry value="0" meaning="High quality pulse B2"/>
          <Entry value="1" meaning="Low quality pulse B2"/>
        </Element>
        <Element name="QD2" bits="1" encoding="table">
          <Entry value="0" meaning="High quality pulse D2"/>
          <Entry value="1" meaning="Low quality pulse D2"/>
        </Element>
        <Element name="QB4" bits="1" encoding="table">
          <Entry value="0" meaning="High quality pulse B4"/>
          <Entry value="1" meaning="Low quality pulse B4"/>
        </Element>
        <Element name="QD4" bits="1" encoding="table">
          <Entry value="0" meaning="High quality pulse D4"/>
          <Entry value="1" meaning="Low quality pulse D4"/>
        </Element>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/110 – Height Measured by a 3D Radar  (Fixed, 2 bytes)
         3DH: signed, LSB = 25 ft
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="110" name="Height Measured by a 3D Radar">
      <Fixed>
        <Spare bits="2"/>
        <Element name="3DH" bits="14" encoding="signed_quantity"
                 scale="25.0" unit="ft"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/120 – Radial Doppler Speed  (Compound)
         Slot 0 (PSF bit7): CAL – Calculated Doppler Speed (Fixed, 2 bytes)
         Slot 1 (PSF bit6): RDS – Raw Doppler Speed (RepetitiveGroup; not
                            supported by the codec – slot marked as unused)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="120" name="Radial Doppler Speed">
      <Compound>
        <!-- slot 0: PSF bit7 – CAL (2 bytes) -->
        <SubItem name="CAL">
          <Fixed>
            <Element name="D" bits="1" encoding="table">
              <Entry value="0" meaning="Doppler speed is valid"/>
              <Entry value="1" meaning="Doppler speed is doubtful"/>
            </Element>
            <Spare bits="5"/>
            <Element name="CAL" bits="10" encoding="signed_quantity"
                     scale="1.0" unit="m/s"/>
          </Fixed>
        </SubItem>
        <!-- slot 1: PSF bit6 – RDS (RepetitiveGroup; not supported, unused) -->
        <SubItem name="-"/>
      </Compound>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/130 – Radar Plot Characteristics  (Compound, 7 × 1-byte sub-items)
         PSF slot mapping: SRL(0), SRR(1), SAM(2), PRL(3), PAM(4), RPD(5), APD(6)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="130" name="Radar Plot Characteristics">
      <Compound>
        <!-- slot 0: PSF bit7 – SRL (SSR Plot Runlength, LSB = 360/2^13 °) -->
        <SubItem name="SRL">
          <Fixed>
            <Element name="SRL" bits="8" encoding="unsigned_quantity"
                     scale="0.043945312" unit="deg"/>
          </Fixed>
        </SubItem>
        <!-- slot 1: PSF bit6 – SRR (Number of Received Replies for MSSR) -->
        <SubItem name="SRR">
          <Fixed>
            <Element name="SRR" bits="8" encoding="raw"/>
          </Fixed>
        </SubItem>
        <!-- slot 2: PSF bit5 – SAM (Amplitude of MSSR Reply, LSB = 1 dBm) -->
        <SubItem name="SAM">
          <Fixed>
            <Element name="SAM" bits="8" encoding="signed_quantity"
                     scale="1.0" unit="dBm"/>
          </Fixed>
        </SubItem>
        <!-- slot 3: PSF bit4 – PRL (Primary Plot Runlength, LSB = 360/2^13 °) -->
        <SubItem name="PRL">
          <Fixed>
            <Element name="PRL" bits="8" encoding="unsigned_quantity"
                     scale="0.043945312" unit="deg"/>
          </Fixed>
        </SubItem>
        <!-- slot 4: PSF bit3 – PAM (Amplitude of Primary Plot, LSB = 1 dBm) -->
        <SubItem name="PAM">
          <Fixed>
            <Element name="PAM" bits="8" encoding="signed_quantity"
                     scale="1.0" unit="dBm"/>
          </Fixed>
        </SubItem>
        <!-- slot 5: PSF bit2 – RPD (Range diff PSR-SSR, LSB = 1/2^8 NM) -->
        <SubItem name="RPD">
          <Fixed>
            <Element name="RPD" bits="8" encoding="signed_quantity"
                     scale="0.00390625" unit="NM"/>
          </Fixed>
        </SubItem>
        <!-- slot 6: PSF bit1 – APD (Azimuth diff PSR-SSR, LSB = 360/2^14 °) -->
        <SubItem name="APD">
          <Fixed>
            <Element name="APD" bits="8" encoding="signed_quantity"
                     scale="0.02197265625" unit="deg"/>
          </Fixed>
        </SubItem>
      </Compound>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/140 – Time of Day  (Fixed, 3 bytes)
         LSB = 1/2^7 = 0.0078125 s
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="140" name="Time of Day">
      <Fixed>
        <Element name="TOD" bits="24" encoding="unsigned_quantity"
                 scale="0.0078125" unit="s" max="86400"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/161 – Track Number  (Fixed, 2 bytes)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="161" name="Track Number">
      <Fixed>
        <Spare bits="4"/>
        <Element name="TRN" bits="12" encoding="raw"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/170 – Track Status  (Extended, up to 2 octets)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="170" name="Track Status">
      <Extended>
        <!-- Octet 1 (7 bits): CNF RAD DOU MAH CDM -->
        <Octet>
          <Element name="CNF" bits="1" encoding="table">
            <Entry value="0" meaning="Confirmed Track"/>
            <Entry value="1" meaning="Tentative Track"/>
          </Element>
          <Element name="RAD" bits="2" encoding="table">
            <Entry value="0" meaning="Combined Track"/>
            <Entry value="1" meaning="PSR Track"/>
            <Entry value="2" meaning="SSR/Mode S Track"/>
            <Entry value="3" meaning="Invalid"/>
          </Element>
          <Element name="DOU" bits="1" encoding="table">
            <Entry value="0" meaning="Normal confidence"/>
            <Entry value="1" meaning="Low confidence in plot to track association"/>
          </Element>
          <Element name="MAH" bits="1" encoding="table">
            <Entry value="0" meaning="No horizontal manoeuvre sensed"/>
            <Entry value="1" meaning="Horizontal manoeuvre sensed"/>
          </Element>
          <Element name="CDM" bits="2" encoding="table">
            <Entry value="0" meaning="Maintaining"/>
            <Entry value="1" meaning="Climbing"/>
            <Entry value="2" meaning="Descending"/>
            <Entry value="3" meaning="Unknown"/>
          </Element>
        </Octet>
        <!-- Octet 2 (7 bits): TRE GHO SUP TCC spare -->
        <Octet>
          <Element name="TRE" bits="1" encoding="table">
            <Entry value="0" meaning="Track still alive"/>
            <Entry value="1" meaning="End of track lifetime (last report for this track)"/>
          </Element>
          <Element name="GHO" bits="1" encoding="table">
            <Entry value="0" meaning="True target track"/>
            <Entry value="1" meaning="Ghost target track"/>
          </Element>
          <Element name="SUP" bits="1" encoding="table">
            <Entry value="0" meaning="No"/>
            <Entry value="1" meaning="Track maintained with info from neighbouring node"/>
          </Element>
          <Element name="TCC" bits="1" encoding="table">
            <Entry value="0" meaning="Tracking performed in Radar Plane"/>
            <Entry value="1" meaning="Slant range correction and projection applied"/>
          </Element>
          <Spare bits="3"/>
        </Octet>
      </Extended>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/200 – Calculated Track Velocity in Polar Co-ordinates  (Fixed, 4 bytes)
         GSP: unsigned, LSB = 1/2^14 NM/s
         HDG: unsigned, LSB = 360/2^16 °
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="200" name="Calculated Track Velocity in Polar Co-ordinates">
      <Fixed>
        <Element name="GSP" bits="16" encoding="unsigned_quantity"
                 scale="0.000061035156" unit="NM/s"/>
        <Element name="HDG" bits="16" encoding="unsigned_quantity"
                 scale="0.0054931640625" unit="deg"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/210 – Track Quality  (Fixed, 4 bytes)
         SIGX, SIGY: unsigned, LSB = 1/2^7 NM
         SIGV:       unsigned, LSB = 1/2^14 NM/s
         SIGH:       unsigned, LSB = 360/2^12 °
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="210" name="Track Quality">
      <Fixed>
        <Element name="SIGX" bits="8" encoding="unsigned_quantity"
                 scale="0.0078125" unit="NM"/>
        <Element name="SIGY" bits="8" encoding="unsigned_quantity"
                 scale="0.0078125" unit="NM"/>
        <Element name="SIGV" bits="8" encoding="unsigned_quantity"
                 scale="0.000061035156" unit="NM/s"/>
        <Element name="SIGH" bits="8" encoding="unsigned_quantity"
                 scale="0.087890625" unit="deg"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/220 – Aircraft Address  (Fixed, 3 bytes)
         24-bit Mode S address.
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="220" name="Aircraft Address">
      <Fixed>
        <Element name="ADR" bits="24" encoding="raw"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/230 – Communications/ACAS Capability and Flight Status  (Fixed, 2 bytes)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="230" name="Communications/ACAS Capability and Flight Status">
      <Fixed>
        <Element name="COM" bits="3" encoding="table">
          <Entry value="0" meaning="No communications capability (surveillance only)"/>
          <Entry value="1" meaning="Comm. A and Comm. B capability"/>
          <Entry value="2" meaning="Comm. A, Comm. B and Uplink ELM"/>
          <Entry value="3" meaning="Comm. A, Comm. B, Uplink ELM and Downlink ELM"/>
          <Entry value="4" meaning="Level 5 Transponder capability"/>
        </Element>
        <Element name="STAT" bits="3" encoding="table">
          <Entry value="0" meaning="No alert, no SPI, aircraft airborne"/>
          <Entry value="1" meaning="No alert, no SPI, aircraft on ground"/>
          <Entry value="2" meaning="Alert, no SPI, aircraft airborne"/>
          <Entry value="3" meaning="Alert, no SPI, aircraft on ground"/>
          <Entry value="4" meaning="Alert, SPI, aircraft airborne or on ground"/>
          <Entry value="5" meaning="No alert, SPI, aircraft airborne or on ground"/>
          <Entry value="7" meaning="Unknown"/>
        </Element>
        <Element name="SI" bits="1" encoding="table">
          <Entry value="0" meaning="SI-Code Capable"/>
          <Entry value="1" meaning="II-Code Capable"/>
        </Element>
        <Spare bits="1"/>
        <Element name="MSSC" bits="1" encoding="table">
          <Entry value="0" meaning="No"/>
          <Entry value="1" meaning="Yes"/>
        </Element>
        <Element name="ARC" bits="1" encoding="table">
          <Entry value="0" meaning="100 ft resolution"/>
          <Entry value="1" meaning="25 ft resolution"/>
        </Element>
        <Element name="AIC" bits="1" encoding="table">
          <Entry value="0" meaning="No"/>
          <Entry value="1" meaning="Yes"/>
        </Element>
        <Element name="B1A" bits="1" encoding="raw"/>
        <Element name="B1B" bits="4" encoding="raw"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/240 – Aircraft Identification  (Fixed, 6 bytes)
         48 bits: 8 × 6-bit ICAO characters (raw; string_icao not in codec)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="240" name="Aircraft Identification">
      <Fixed>
        <Element name="IDENT" bits="48" encoding="raw"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/250 – BDS Register Data  (Repetitive count-prefixed)
         REP(1B) + REP × { MBDATA(56b) + BDS1(4b) + BDS2(4b) } = REP × 8B
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="250" name="BDS Register Data">
      <Repetitive type="count" count_bytes="1">
        <Group>
          <Element name="MBDATA" bits="56" encoding="raw"/>
          <Element name="BDS1"   bits="4"  encoding="raw"/>
          <Element name="BDS2"   bits="4"  encoding="raw"/>
        </Group>
      </Repetitive>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         I048/260 – ACAS Resolution Advisory Report  (Fixed, 7 bytes)
         56-bit raw field per BDS register 3,0.
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="260" name="ACAS Resolution Advisory Report">
      <Fixed>
        <Element name="ACASRA" bits="56" encoding="raw"/>
      </Fixed>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         RE – Reserved Expansion Field  (Explicit: first byte = total length)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="RE" name="Reserved Expansion Field">
      <Explicit type="re"/>
    </DataItem>

    <!-- ══════════════════════════════════════════════════════════════════════
         SP – Special Purpose Field  (Explicit: first byte = total length)
         ═════════════════════════════════════════════════════════════════════ -->
    <DataItem id="SP" name="Special Purpose Field">
      <Explicit type="sp"/>
    </DataItem>

  </DataItems>

  <!-- ════════════════════════════════════════════════════════════════════════
       UAP (User Application Profile) – single variation, no discriminator.
       28 slots across 4 FSPEC bytes (7 slots each).
       "-" = unused FSPEC slot.
       ════════════════════════════════════════════════════════════════════════ -->
  <UAPs default="default">

    <Variation name="default">
      <!-- FSPEC byte 1: slots 1–7 -->
      <Item ref="010"/>  <!-- slot  1: FSPEC byte 1, bit 7 -->
      <Item ref="140"/>  <!-- slot  2: FSPEC byte 1, bit 6 -->
      <Item ref="020"/>  <!-- slot  3: FSPEC byte 1, bit 5 -->
      <Item ref="040"/>  <!-- slot  4: FSPEC byte 1, bit 4 -->
      <Item ref="070"/>  <!-- slot  5: FSPEC byte 1, bit 3 -->
      <Item ref="090"/>  <!-- slot  6: FSPEC byte 1, bit 2 -->
      <Item ref="130"/>  <!-- slot  7: FSPEC byte 1, bit 1 -->
      <!-- FSPEC byte 2: slots 8–14 -->
      <Item ref="220"/>  <!-- slot  8: FSPEC byte 2, bit 7 -->
      <Item ref="240"/>  <!-- slot  9: FSPEC byte 2, bit 6 -->
      <Item ref="250"/>  <!-- slot 10: FSPEC byte 2, bit 5 -->
      <Item ref="161"/>  <!-- slot 11: FSPEC byte 2, bit 4 -->
      <Item ref="042"/>  <!-- slot 12: FSPEC byte 2, bit 3 -->
      <Item ref="200"/>  <!-- slot 13: FSPEC byte 2, bit 2 -->
      <Item ref="170"/>  <!-- slot 14: FSPEC byte 2, bit 1 -->
      <!-- FSPEC byte 3: slots 15–21 -->
      <Item ref="210"/>  <!-- slot 15: FSPEC byte 3, bit 7 -->
      <Item ref="030"/>  <!-- slot 16: FSPEC byte 3, bit 6 -->
      <Item ref="080"/>  <!-- slot 17: FSPEC byte 3, bit 5 -->
      <Item ref="100"/>  <!-- slot 18: FSPEC byte 3, bit 4 -->
      <Item ref="110"/>  <!-- slot 19: FSPEC byte 3, bit 3 -->
      <Item ref="120"/>  <!-- slot 20: FSPEC byte 3, bit 2 -->
      <Item ref="230"/>  <!-- slot 21: FSPEC byte 3, bit 1 -->
      <!-- FSPEC byte 4: slots 22–28 -->
      <Item ref="260"/>  <!-- slot 22: FSPEC byte 4, bit 7 -->
      <Item ref="055"/>  <!-- slot 23: FSPEC byte 4, bit 6 -->
      <Item ref="050"/>  <!-- slot 24: FSPEC byte 4, bit 5 -->
      <Item ref="065"/>  <!-- slot 25: FSPEC byte 4, bit 4 -->
      <Item ref="060"/>  <!-- slot 26: FSPEC byte 4, bit 3 -->
      <Item ref="SP"/>   <!-- slot 27: FSPEC byte 4, bit 2 -->
      <Item ref="RE"/>   <!-- slot 28: FSPEC byte 4, bit 1 -->
    </Variation>

  </UAPs>

</Category>
